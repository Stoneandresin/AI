# Path to the Google service account JSON file used for Sheets and Vision access
GOOGLE_SERVICE_ACCOUNT_JSON=/path/to/service_account.json

# Google Sheet ID that stores inventory and catalog tabs
GOOGLE_SHEETS_INVENTORY_SHEET_ID=your_sheet_id_here

# Optional overrides
# GOOGLE_SHEETS_INVENTORY_TAB=Inventory
# GOOGLE_SHEETS_CATALOG_TAB=Catalog
# ZAPIER_WEBHOOK_SECRET=supersecret
# DEFAULT_MIN_QUANTITY=1
# RESTOCK_QUANTITY=10
# ALLOW_UNKNOWN_LABELS=true
